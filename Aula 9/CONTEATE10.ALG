algoritmo "Conteate10"
var
   contador, n1, salto: inteiro
inicio
      contador <- 0
      Escreva ("Conte de 0 até ")
      Leia (n1)
      Escreva ("Qual será o valor do salto? ")
      Leia (salto)
      Enquanto (contador <= n1 ) faca
               Escreval (contador)
               contador <- contador + salto
      FimEnquanto
      Escreval ("Terminei de contar")
fimalgoritmo