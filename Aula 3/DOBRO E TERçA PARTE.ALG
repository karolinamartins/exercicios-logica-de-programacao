algoritmo "dobro e terça parte"
var
   n1: real
   d: real
   t: real
inicio
      Escreval("Olá! Bem vindo(a).")
      Escreva("Digite um número e informaremos seu dobro e terça parte: ")
      Leia (n1)
      d <- n1*2
      t <- n1/3
      Escreva ("O dobro de", n1, " é ", d, ". E seu terço é ", t)
fimalgoritmo